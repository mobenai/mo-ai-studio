{
  "name": "mo-ai-studio",
  "private": true,
  "productName": "mo-ai-studio",
  "version": "1.5.1",
  "description": "mo ai studio local ai agent for everyone",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start -- --dev",
    "make:debug": "electron-forge make --arch=\"arm64,x64\" --debug",
    "package": "electron-forge package",
    "make": "cross-env $(cat .env | xargs) electron-forge make --arch=\"arm64,x64\"",
    "make:darwin": "cross-env $(cat .env | xargs) electron-forge make --arch=\"arm64,x64\" --platform=darwin",
    "make:win32": "cross-env $(cat .env | xargs) electron-forge make --arch=\"arm64,x64\" --platform=win32",
    "make:all": "npm run make:darwin && npm run make:win32",
    "publish:darwin": "cross-env $(cat .env | xargs) electron-forge publish --arch=\"arm64,x64\" --platform=darwin",
    "publish:win32": "cross-env $(cat .env | xargs) electron-forge publish --arch=\"arm64,x64\" --platform=win32",
    "publish:all": "rm -rf ./out && npm run publish:darwin && npm run publish:win32",
    "lint": "eslint --ext .ts,.tsx .",
    "git": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinop112365362/mo-ai-stuidio.git"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-pkg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "javascript-obfuscator": "^4.1.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "Shi Hong",
    "email": "shihong@mobenai.com.cn"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "electron-log": "^5.2.0",
    "electron-screenshots": "^0.5.27",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.4",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "ignore": "^5.3.2",
    "jimp": "^1.6.0",
    "node-fetch": "2",
    "simple-git": "^3.26.0",
    "update-electron-app": "^3.0.0",
    "ws": "^8.18.0"
  }
}
